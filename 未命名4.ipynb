   "source": [
    "def focal_loss(alpha, gamma):\n",
    "    def focal_loss_func(y_pred, y_true):\n",
    "        p_t = np.exp(-y_pred)\n",
    "        loss = alpha * (1 - p_t) ** gamma * y_pred\n",
    "        return \"focal_loss\", np.mean(loss)\n",
    "    return focal_loss_func\n"
   ]
  }
